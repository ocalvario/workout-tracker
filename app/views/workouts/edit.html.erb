<h1>Edit Your Workout</h1>

<form action="/workouts/<%= @workout.id %>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label> New Workout Name </label>
  <input type="text" value="<%=@workout.name %>" name="name"> </br>

  <label> New Workout Date </label>
  <input type="text" value="<%=@workout.date %>"name="date"> </br>

  <input type="submit" value="Create">
</form>
